{% block _text_id_widget %}
{% spaceless %}
    <div id="container">
        {{ form_widget(form) }}
    </div>
{% endspaceless %}
{% endblock _text_id_widget %}

{% block _name_entry_label %}
{% spaceless %}
    {% if label is empty %}
        {% set label = name|humanize %}
    {% endif %}
    <label>Custom label: {{ label|trans({}, translation_domain) }}</label>
{% endspaceless %}
{% endblock _name_entry_label %}
